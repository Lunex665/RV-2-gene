      <li Class="RimVore2.GeneDefExtension_ForcedHediff">
        <hediffs>
          <li>RV2_TypeEnabler_Tailmaw</li>
        </hediffs>
        <removeHediffsOnGeneRemoval>true</removeHediffsOnGeneRemoval>
      </li>
      <customEffectDescriptions>
        <li>The owner of this gene can open a mouth on a tail</li>
      </customEffectDescriptions>
    </modExtensions>
  </GeneDef>

  <GeneDef ParentName="RV2_VoreEnabler">
    <defName>RV2_TypeEnabler_Udder</defName>
    <label>Udder elasticity</label>
    <iconPath>UI/Icons/Genes/Udder</iconPath>
    <modExtensions>
      <li Class="RimVore2.GeneDefExtension_ForcedHediff">
        <hediffs>
          <li>RV2_TypeEnabler_Udder</li>
        </hediffs>
        <removeHediffsOnGeneRemoval>true</removeHediffsOnGeneRemoval>
      </li>
      <customEffectDescriptions>
        <li>The owner of this gene has a udder that can be dilated</li>
      </customEffectDescriptions>
    </modExtensions>
  </GeneDef>

</Defs>